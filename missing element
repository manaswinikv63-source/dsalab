#include <iostream>
using namespace std;

int main()
{
    int N;
    cin >> N;
    int arr[N-1];
    int sum = 0;
    int i;

    for (i = 0; i < N - 1; i++)
    {
        cin >> arr[i];
        sum = sum + arr[i];
    }

    int totalsum = N * (N + 1) / 2;
    cout << totalsum - sum;
    return 0;
}
